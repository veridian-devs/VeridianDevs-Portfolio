{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"git": {
		"productionBranch": "main",
		"deploymentEnabled": {
			"main": true,
			"*": false
		}
	},
	"github": {
		"autoAlias": true,
		"autoJobCancelation": true
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	]
}
